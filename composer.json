{
  "name": "photon-php/framework",
  "description": "The Photon PHP Framework.",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/photon-php/framework/issues",
    "source": "https://github.com/photon-php/framework"
  },
  "authors": [
    {
      "role": "Developer",
      "name": "Steve McDougall",
      "email": "juststevemcd@gmail.com",
      "homepage": "https://www.juststeveking.uk/"
    }
  ],
  "require": {
    "php": "^8.3",
    "composer-runtime-api": "^2.2",
    "illuminate/auth": "^10.43",
    "illuminate/broadcasting": "^10.43",
    "illuminate/bus": "^10.43",
    "illuminate/cache": "^10.43",
    "illuminate/collections": "^10.43",
    "illuminate/config": "^10.43",
    "illuminate/console": "^10.43",
    "illuminate/container": "^10.43",
    "illuminate/contracts": "^10.43",
    "illuminate/database": "^10.43",
    "illuminate/encryption": "^10.43",
    "illuminate/events": "^10.43",
    "illuminate/filesystem": "^10.43",
    "illuminate/hashing": "^10.43",
    "illuminate/http": "^10.43",
    "illuminate/macroable": "^10.43",
    "illuminate/pagination": "^10.43",
    "illuminate/pipeline": "^10.43",
    "illuminate/queue": "^10.43",
    "illuminate/support": "^10.43",
    "illuminate/testing": "^10.43",
    "illuminate/translation": "^10.43",
    "illuminate/validation": "^10.43",
    "illuminate/view": "^10.43",
    "illuminate/log": "^10.43",
    "dragonmantank/cron-expression": "^3.3.3",
    "nikic/fast-route": "^1.3",
    "symfony/console": "^6.4.3",
    "symfony/error-handler": "^6.4.3",
    "symfony/http-kernel": "^6.4.3",
    "symfony/http-foundation": "^6.4.3",
    "symfony/mime": "^6.4.3",
    "symfony/var-dumper": "^6.4.3",
    "vlucas/phpdotenv": "^5.6.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.6.7",
    "phpunit/phpunit": "10.5.3",
    "roave/security-advisories": "dev-latest"
  },
  "suggest": {
    "laravel/tinker": "Required to use the tinker console command (^2.7).",
    "nyholm/psr7": "Required to use PSR-7 bridging features (^1.2).",
    "symfony/psr-http-message-bridge": "Required to use PSR-7 bridging features (^2.0)."
  },
  "autoload": {
    "psr-4": {
      "Photon\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
